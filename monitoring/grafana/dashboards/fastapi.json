{
  "uid": "fastapi-overview",
  "title": "FastAPI Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP Request Rate (RPS)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "rps",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP P95 Latency (s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "In-Flight Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "fastapi_inprogress",
          "legendFormat": "inflight",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "WebSocket Connections",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(app_ws_connections) by (channel)",
          "legendFormat": "{{channel}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Process Memory (bytes)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "rss",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
    }
  ]
}
